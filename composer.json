{
  "name": "cycle/annotated-entities",
  "type": "library",
  "description": "Cycle ORM Annotated Entities generator",
  "require": {
    "php": "^7.1|^8.0",
    "cycle/orm": "dev-master",
    "cycle/schema-builder": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1",
    "spiral/tokenizer": "^2.1",
    "spiral/debug": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Cycle\\Annotated\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\Annotated\\Tests\\": "tests/Annotated/"
    }
  }
}
